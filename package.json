{
  "name": "blot",
  "version": "0.0.18",
  "description": "DRY documentation builder",
  "main": "src/blot.js",
  "scripts": {
    "build": "node ./node_modules/.bin/babel src --out-dir lib && ./node_modules/.bin/babel bin/blot --out-dir exec",
    "lint": "eslint src/blot.js",
    "start": "./node_modules/.bin/babel-node src/blot.js",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/mocha --reporter nyan --compilers js:babel/register test",
    "auto-start": "nodemon --exec \"npm run lint && npm start\" --ignore public/js",
    "preinstall": "npm install babel@5.8.21",
    "postinstall": "npm run build",
    "prepublish": "npm run build"
  },
  "bin": {
    "blot": "./exec/bin/blot.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/slurmulon/blot.git"
  },
  "keywords": [
    "api",
    "blueprint",
    "md",
    "markdown",
    "build",
    "fixture"
  ],
  "author": "MadHax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slurmulon/blot/issues"
  },
  "dependencies": {
    "aglio": "^2.2.0",
    "bunyan": "^1.5.1",
    "bunyan-prettystream": "^0.1.3",
    "cheerio": "^0.19.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "config.json": "0.0.4",
    "glob": "^5.0.10",
    "hazy": "^0.1.7",
    "hercule": "^1.2.1",
    "jsonpath": "^0.1.4",
    "lodash": "~3.9.3",
    "mkpath": "^1.0.0",
    "path": "^0.12.7",
    "protagonist": "^1.1.1"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "babel-eslint": "^4.0.10",
    "blanket": "~1.1.7",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "chai-things": "^0.2.0",
    "eslint": "^1.1.0",
    "mocha": "^2.3.2",
    "nodemon": "^1.4.1"
  }
}
