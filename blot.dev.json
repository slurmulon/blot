{
  "name": "dev",
  "host": "http://example.blot.apps.madhax.io",
  "base": ".",
  "docs": {
    "src": "src/docs",
    "dest": "dist/docs",
    "export": true
  },
  "fixtures": {
    "src": "src/fixtures",
    "dest": "dist/fixtures",
    "export": false
  },
  "view": {
    "dest": "dist/api.blot.html",
    "export": true,
    "options": {
      "themeFullWidth": true,
      "themeVariables": "slate"
    },
    "elements": {
      "container": "body",
      "scripts": false,
      "comments": false
    },
    "attrs": {}
  },
  "meta": {
    "src": "src/meta"
  },
  "logging": true,
  "pretty": false
}